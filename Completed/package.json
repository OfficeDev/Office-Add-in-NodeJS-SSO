    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.11",
    "moment": "2.23.0",
    "morgan": "1.9.1",
    "node-persist": "3.0.2",
    "nodemon": "1.18.9",
    "uuid": "3.3.2"
  },
  "devDependencies": {
